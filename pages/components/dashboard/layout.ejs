        </div>
    </nav>
    <div class="w-full">
        <div class="bg-[var(--primary-color)] w-full h-full flex relative">
            <div class="mt-28 mx-96 w-full">
                <%- include(`pages/${dashboardPage}.ejs`) %>
            </div>
        </div>
    </div>
</div>
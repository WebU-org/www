                <div class="flex items-center justify-between w-full mb-2">
                    <div style="position: relative;" class="w-full">
                        <input class="dropdown" type="checkbox" id="dropdown-link-${link.id}" name="dropdown"/>
                        <label style="display: block;" class="link-dropdown for-dropdown" for="dropdown-link-${link.id}">
                            <div class="py-2 px-3 font-semibold cursor-pointer rounded select-none text-sm border-primary">
                                <%= user.email %>
                            </div>
                        </label>
                        <div style="margin-bottom: 10px;" class="section-dropdown !w-full">
                            <a href="#" onclick="setTheme(!(localStorage.getItem(THEME_SELECTOR) === '1'))">
                                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 21a4 4 0 01-4-4V5a2 2 0 012-2h4a2 2 0 012 2v12a4 4 0 01-4 4zm0 0h12a2 2 0 002-2v-4a2 2 0 00-2-2h-2.343M11 7.343l1.657-1.657a2 2 0 012.828 0l2.829 2.829a2 2 0 010 2.828l-8.486 8.485M7 17h.01"></path></svg>
